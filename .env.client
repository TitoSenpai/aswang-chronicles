# Client Production Environment Variables

# Admin Configuration - Client Production
VITE_ADMIN_KEY=client_admin_key_here

# Firebase Configuration - Client Production Values
# Note: Client needs to create their own Firebase project
VITE_FIREBASE_API_KEY=client_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=client-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=client-project-id
VITE_FIREBASE_STORAGE_BUCKET=client-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=client_sender_id
VITE_FIREBASE_APP_ID=client_app_id

# Application Settings - Client Production
VITE_APP_NAME=Aswang Chronicles
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Moderation Settings - Production Values
VITE_AUTO_APPROVAL_THRESHOLD=80
VITE_AUTO_REJECTION_THRESHOLD=20
VITE_RATE_LIMIT_COMMENTS_PER_HOUR=5

# Security Settings - Production Values
VITE_SESSION_TIMEOUT_MINUTES=60
VITE_MAX_LOGIN_ATTEMPTS=5